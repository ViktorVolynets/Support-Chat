<link href="~/css/AdminStats.css" rel="stylesheet" type="text/css" />


<div class="form-group">
    <div class="form-group">
        <a asp-controller="Admin" asp-action="ErrorLogs">Error Logs</a>
    </div>
    <div class="form-group">
        <a asp-controller="Admin" asp-action="TraceLogs">Trace Logs</a>
    </div>

    <hr>
</div>


<div class="form-group">
    <div>
        <div class="text-info">
            <label>Active users :</label>
        </div>
        <div class="form-text">
            <label class="admin-stats">@ViewData["ActiveUsers"]</label>
        </div>
        
    </div>

    <div>
        <div class="text-info">
            <label>Active operators :</label>
        </div>
        <div class="form-text">
            <label class="admin-stats">@ViewData["ActiveOperators"]</label>
        </div>
    </div>
</div>
